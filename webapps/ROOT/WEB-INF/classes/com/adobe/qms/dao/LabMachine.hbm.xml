<hibernate-mapping>
    <class name="com.adobe.qms.dao.LabMachine" table="lab_machine">
        <id name="lmId" type="java.lang.Integer">
            <column name="lm_id" />
            <generator class="increment" />
        </id>
        <many-to-one name="user" class="com.adobe.qms.dao.User" fetch="select">
            <column name="user_id" length="45" />
        </many-to-one>
        <many-to-one name="machine" class="com.adobe.qms.dao.Machine" fetch="select">
            <column name="address" length="45" not-null="true" unique="true" />
        </many-to-one>
        <property name="status" type="java.lang.Integer">
            <column name="status" not-null="true" />
        </property>
        <property name="isBook" type="java.lang.Integer">
            <column name="is_book" not-null="true" />
        </property>
        <property name="bookTime" type="java.lang.Integer">
            <column name="book_time" not-null="true" />
        </property>
        <property name="note" type="java.lang.String">
            <column name="note" length="1024" />
        </property>
    </class>
</hibernate-mapping>
