<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    Mapping file generated by Jacky Li (yxli@adobe.com)
-->
<hibernate-mapping>
    <class name="com.adobe.qms.dao.AutoSwEnvironment" table="auto_sw_environment">
        <id name="envId" type="java.lang.Integer">
            <column name="env_id" />
            <generator class="increment" />
        </id>
        <property name="software" type="java.lang.String">
            <column name="software" length="33" not-null="true" />
        </property>
        <property name="sfLocation" type="java.lang.String">
            <column name="sf_location" length="100">
                <comment>the software location</comment>
            </column>
        </property>
        <set name="autoJobResults" inverse="true">
            <key>
                <column name="env_id" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.AutoJobResult" />
        </set>
    </class>
</hibernate-mapping>
