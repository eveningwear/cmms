<hibernate-mapping>
    <class name="com.adobe.qms.dao.Cloud" table="cloud">
        <id name="cloudId" type="java.lang.Integer">
            <column name="cloud_id" />
            <generator class="increment" />
        </id>
        <many-to-one name="user" class="com.adobe.qms.dao.User" fetch="select">
            <column name="user_id" length="45" not-null="true" />
        </many-to-one>
        <property name="cloudName" type="java.lang.String">
            <column name="cloud_name" length="45" not-null="true" />
        </property>
        <property name="type" type="java.lang.String">
            <column name="type" length="16" not-null="true" />
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="create_time" length="19" not-null="true" />
        </property>
        <set name="cloudLists" inverse="true">
            <key>
                <column name="cloud_id" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.CloudList" />
        </set>
    </class>
</hibernate-mapping>
