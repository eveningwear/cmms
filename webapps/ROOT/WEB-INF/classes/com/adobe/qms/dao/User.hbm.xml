<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    Mapping file generated by Jacky Li (yxli@adobe.com)
-->
<hibernate-mapping>
    <class name="com.adobe.qms.dao.User" table="user">
        <id name="userId" type="java.lang.String">
            <column name="user_id" length="45" />
            <generator class="assigned" />
        </id>
        <many-to-one name="productTeam" class="com.adobe.qms.dao.ProductTeam" fetch="select">
            <column name="team_name" length="45" />
        </many-to-one>
        <property name="lastLogonTime" type="java.util.Date">
            <column name="last_logon_time" length="19" />
        </property>
        <property name="password" type="java.lang.String">
            <column name="password" length="45" not-null="true" />
        </property>
        <set name="labMachines" inverse="true">
            <key>
                <column name="user_id" length="45" />
            </key>
            <one-to-many class="com.adobe.qms.dao.LabMachine" />
        </set>
        <set name="jobs" inverse="true">
            <key>
                <column name="user_id" length="45" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.Job" />
        </set>
        <set name="clouds" inverse="true">
            <key>
                <column name="user_id" length="45" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.Cloud" />
        </set>
        <set name="machineLists" inverse="true">
            <key>
                <column name="user_id" length="45" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.MachineList" />
        </set>
        <set name="machines" inverse="true">
            <key>
                <column name="user_id" length="45" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.Machine" />
        </set>
    </class>
</hibernate-mapping>
