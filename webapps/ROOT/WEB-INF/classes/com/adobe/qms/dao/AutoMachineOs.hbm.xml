<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    Mapping file generated by Jacky Li (yxli@adobe.com)
-->
<hibernate-mapping>
    <class name="com.adobe.qms.dao.AutoMachineOs" table="auto_machine_os">
        <id name="osId" type="java.lang.Integer">
            <column name="os_id" />
            <generator class="increment" />
        </id>
        <property name="osType" type="java.lang.String">
            <column name="os_type" length="50" not-null="true" />
        </property>
        <property name="edition" type="java.lang.String">
            <column name="edition" length="10" not-null="true">
                <comment>such as Home edition for windows, 10.5.4 for Leopard</comment>
            </column>
        </property>
        <property name="osLang" type="java.lang.String">
            <column name="os_lang" length="10" not-null="true">
                <comment>such as en_US, english, En</comment>
            </column>
        </property>
        <set name="autoMachineImages" inverse="true">
            <key>
                <column name="os_id" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.AutoMachineImage" />
        </set>
    </class>
</hibernate-mapping>
