<hibernate-mapping>
    <class name="com.adobe.qms.dao.AutoMachineModel" table="auto_machine_model">
        <id name="modelId" type="java.lang.Integer">
            <column name="model_id" />
            <generator class="increment" />
        </id>
        <property name="model" type="java.lang.String">
            <column name="model" length="45" not-null="true" />
        </property>
        <property name="osCategory" type="java.lang.String">
            <column name="os_category" length="15" not-null="true" />
        </property>
        <set name="autoMachineImages" inverse="true">
            <key>
                <column name="model_id" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.AutoMachineImage" />
        </set>
        <set name="autoMachines" inverse="true">
            <key>
                <column name="model_id" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.AutoMachine" />
        </set>
    </class>
</hibernate-mapping>
