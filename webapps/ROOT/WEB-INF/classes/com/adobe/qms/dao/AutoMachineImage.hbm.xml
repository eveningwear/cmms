<hibernate-mapping>
    <class name="com.adobe.qms.dao.AutoMachineImage" table="auto_machine_image">
        <id name="imageId" type="java.lang.Integer">
            <column name="image_id" />
            <generator class="increment" />
        </id>
        <many-to-one name="autoMachineModel" class="com.adobe.qms.dao.AutoMachineModel" fetch="select">
            <column name="model_id" not-null="true" />
        </many-to-one>
        <many-to-one name="autoMachineOs" class="com.adobe.qms.dao.AutoMachineOs" fetch="select">
            <column name="os_id" not-null="true" />
        </many-to-one>
        <property name="imageLocation" type="java.lang.String">
            <column name="image_location" length="100" not-null="true" />
        </property>
        <set name="autoJobResults" inverse="true">
            <key>
                <column name="image_id" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.AutoJobResult" />
        </set>
    </class>
</hibernate-mapping>
