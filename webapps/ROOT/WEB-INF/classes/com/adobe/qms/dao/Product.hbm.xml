<hibernate-mapping>
    <class name="com.adobe.qms.dao.Product" table="product">
        <id name="productId" type="java.lang.Integer">
            <column name="product_id" />
            <generator class="increment" />
        </id>
        <property name="productName" type="java.lang.String">
            <column name="product_name" length="45" not-null="true" />
        </property>
        <property name="version" type="java.lang.String">
            <column name="version" length="45" not-null="true" />
        </property>
        <property name="platform" type="java.lang.String">
            <column name="platform" length="45" not-null="true" />
        </property>
        <property name="language" type="java.lang.String">
            <column name="language" length="45" not-null="true" />
        </property>
        <property name="certLevel" type="java.lang.String">
            <column name="cert_level" length="45" not-null="true" />
        </property>
        <property name="subProduct" type="java.lang.String">
            <column name="sub_product" length="45" not-null="true" />
        </property>
        <set name="latestBuilds" inverse="true">
            <key>
                <column name="product_id" not-null="true" unique="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.LatestBuild" />
        </set>
        <set name="builds" inverse="true">
            <key>
                <column name="product_id" not-null="true" unique="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.Build" />
        </set>
    </class>
</hibernate-mapping>
