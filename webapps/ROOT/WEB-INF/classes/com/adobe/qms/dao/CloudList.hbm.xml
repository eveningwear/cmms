<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    Mapping file generated by Jacky Li (yxli@adobe.com)
-->
<hibernate-mapping>
    <class name="com.adobe.qms.dao.CloudList" table="cloud_list">
        <id name="clId" type="java.lang.Integer">
            <column name="cl_id" />
            <generator class="increment" />
        </id>
        <many-to-one name="machine" class="com.adobe.qms.dao.Machine" fetch="select">
            <column name="address" length="45" not-null="true" />
        </many-to-one>
        <many-to-one name="cloud" class="com.adobe.qms.dao.Cloud" fetch="select">
            <column name="cloud_id" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>
