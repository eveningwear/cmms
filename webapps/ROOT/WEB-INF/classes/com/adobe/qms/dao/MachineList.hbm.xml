<hibernate-mapping>
    <class name="com.adobe.qms.dao.MachineList" table="machine_list">
        <id name="mlId" type="java.lang.Integer">
            <column name="ml_id" />
            <generator class="increment" />
        </id>
        <many-to-one name="user" class="com.adobe.qms.dao.User" fetch="select">
            <column name="user_id" length="45" not-null="true" />
        </many-to-one>
        <many-to-one name="machine" class="com.adobe.qms.dao.Machine" fetch="select">
            <column name="machine_id" length="45" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>
