<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    Mapping file generated by Jacky Li (yxli@adobe.com)
-->
<hibernate-mapping>
    <class name="com.adobe.qms.dao.ProductTeam" table="product_team">
        <id name="teamName" type="java.lang.String">
            <column name="team_name" length="45" />
            <generator class="assigned" />
        </id>
        <property name="managerName" type="java.lang.String">
            <column name="manager_name" length="45" not-null="true" />
        </property>
        <set name="machines" inverse="true">
            <key>
                <column name="team_name" length="45" />
            </key>
            <one-to-many class="com.adobe.qms.dao.Machine" />
        </set>
        <set name="users" inverse="true">
            <key>
                <column name="team_name" length="45" />
            </key>
            <one-to-many class="com.adobe.qms.dao.User" />
        </set>
    </class>
</hibernate-mapping>
