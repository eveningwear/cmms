<hibernate-mapping>
    <class name="com.adobe.qms.dao.AutoMachine" table="auto_machine">
        <id name="autoMachineId" type="java.lang.Integer">
            <column name="auto_machine_id" />
            <generator class="increment" />
        </id>
        <many-to-one name="machine" class="com.adobe.qms.dao.Machine" fetch="select">
            <column name="address" length="45" not-null="true" />
        </many-to-one>
        <many-to-one name="autoMachineModel" class="com.adobe.qms.dao.AutoMachineModel" fetch="select">
            <column name="model_id" not-null="true" />
        </many-to-one>
        <property name="startTime" type="java.util.Date">
            <column name="start_time" length="19" not-null="true" />
        </property>
        <property name="tactic" type="java.lang.Integer">
            <column name="tactic" not-null="true" />
        </property>
        <set name="autoJobResults" inverse="true">
            <key>
                <column name="auto_machine_id" not-null="true" />
            </key>
            <one-to-many class="com.adobe.qms.dao.AutoJobResult" />
        </set>
    </class>
</hibernate-mapping>
